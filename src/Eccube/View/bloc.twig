{% if blocks|length > 0 %}
        {% for block in blocks %}
            <!-- ▼{{ block.name }} -->
            {% if block.phpPath %}
                {{ render(path('block_' ~ block.filename)) }}
            {% else %}
            	{{ include('frontparts/bloc/' ~ block.tplPath) }}
            {% endif %}
            <!-- ▲{{ block.name }} -->
        {% endfor %}
{% endif %}